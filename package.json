{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "aws-amplify": "^0.2.14",
    "aws-amplify-react": "^0.1.38",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
